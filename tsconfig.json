{
  "compilerOptions": {
    "jsx": "react-jsx",
    "types": ["react", "react-dom"],
    "lib": ["dom", "dom.iterable", "esnext", "es2015"],
    "esModuleInterop": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "bundler",
    "module": "esnext",
    "target": "esnext",
    "skipLibCheck": true
  },

  "include": ["src/**/*","svg.d.ts"],
}